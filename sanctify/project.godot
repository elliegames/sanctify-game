; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Sanctify"
config/description="Get rid of curses placed behind floors of the Temple"
config/version="0.0.1"
run/main_scene="res://main_menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="sanctify"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=30
run/low_processor_mode=true
boot_splash/bg_color=Color(0.666667, 0.498039, 0.313726, 1)
boot_splash/image="res://data/images/boot_splash.png"
config/icon="res://icon.png"
boot_splash/minimum_display_time=500

[audio]

general/3d_panning_strength=1.0
driver/output_latency=50

[autoload]

PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
Audio="*res://audio.tscn"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/window_width_override=800
window/size/window_height_override=600

[dotnet]

project/assembly_name="Sanctify"

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

